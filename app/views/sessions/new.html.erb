<h1>Log In</h1>

<%= form_tag session_path do %>
    <div>
        <%= label_tag :email %>
        <%= email_field_tag :email %>  
    </div>
    <div>
        <%= label_tag :password %>
        <%= password_field_tag :password %>  
    </div>
    <div>
        <%= submit_tag "Log In" %> 
    </div>
<% end %>  

<h1>Do not have an account? Sign Up:</h1>
<%= link_to "Sign Up", new_user_path %> 